{
  "cli": {
    "version": ">= 2.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "review": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "review"
    },
    "staging": {
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "API_URL": "",
        "BUILD_ENV": "staging",
        "ROLLBAR_ACCESS_TOKEN": "",
        "SENTRY_PROJECT_NAME": "",
        "SENTRY_DSN": ""
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "API_URL": "",
        "BUILD_ENV": "production",
        "ROLLBAR_ACCESS_TOKEN": "",
        "SENTRY_PROJECT_NAME": "",
        "SENTRY_DSN": ""
      }
    }
  }
}