{
  "project_name": "My Project",
  "author_name": "ThinkNimble",
  "email": "hello@thinknimble.com",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_')|replace('.', '_')|trim() }}",
  "_extensions": [
    "jinja2_time.TimeExtension",
    "cookiecutter.extensions.RandomStringExtension"
  ],
  "_copy_without_render": [
    "clients/web/vue3/src/views/dashboard",
    "clients/web/vue3/src/components",
    "clients/web/react/src/components",
    "clients/web/react/src/pages/app-or-auth.tsx",
    "clients/web/react/src/pages/home.tsx",
    "clients/web/react/src/pages/index.ts",
    "clients/web/react/src/pages/layout.tsx",
    "*/swagger-ui.html"
  ],
  "mail_service": [
    "Mailgun",
    "Amazon SES",
    "Custom SMTP"
  ],
  "client_app": [
    "Vue3",
    "React",
    "None"
  ],
  "include_mobile": "n",
  "include_services_core": "n",
  "__prompts__": {
    "project_name": "Project name:",
    "author_name": "Author:",
    "email": "Email:",
    "project_slug": "The project slug",
    "mail_service": "Choose an email service to use",
    "client_app": "Choose a web frontend client",
    "include_mobile": "Want to add a mobile project? (y/n)",
    "include_services_core": "Want to make this a JS monorepo and share code across clients? (requires both web and mobile clients) (y/n)"
  }
}